<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cafesystem.CafeController">
   <children>
      <Label layoutX="95.0" layoutY="14.0" prefHeight="27.0" prefWidth="110.0" text="Add Tea Type">
