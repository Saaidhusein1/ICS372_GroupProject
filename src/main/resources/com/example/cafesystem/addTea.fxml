            <Font size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="comb2" layoutX="75.0" layoutY="41.0" prefWidth="150.0" />
      <ComboBox fx:id="comb3" layoutX="75.0" layoutY="93.0" prefWidth="150.0" />
      <ComboBox fx:id="comb4" layoutX="75.0" layoutY="150.0" prefWidth="150.0" />
      <Label layoutX="110.0" layoutY="66.0" prefHeight="27.0" prefWidth="80.0" text="Add Size">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="70.0" layoutY="122.0" prefHeight="27.0" prefWidth="160.0" text="Add Customization">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="70.0" layoutY="191.0" mnemonicParsing="false" onAction="#addTeaToOrder" prefHeight="43.0" prefWidth="160.0" text="Click to Add Quantity">
